# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Tab indentation (no size specified)
[*.{ado,do,mata}]
indent_style = tab
insert_final_newline = true

# Tab indentation (no size specified)
[*.{sh,py}]
indent_style = tab

[{makefile}]
indent_style = tab
